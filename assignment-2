# Base class
class Vehicle:
    def move(self):
        raise NotImplementedError("Subclass must implement abstract method")

# Subclass for Car
class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

# Subclass for Plane
class Plane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

# Subclass for Boat
class Boat(Vehicle):
    def move(self):
        print("Sailing ğŸš¤")

# Subclass for Bicycle
class Bicycle(Vehicle):
    def move(self):
        print("Pedaling ğŸš²")

# Function to demonstrate polymorphism
def demonstrate_move(vehicle):
    vehicle.move()

# Create objects of different vehicle types
car = Car()
plane = Plane()
boat = Boat()
bicycle = Bicycle()

# Demonstrate the move method in action
demonstrate_move(car)      # Output: Driving ğŸš—
demonstrate_move(plane)    # Output: Flying âœˆï¸
demonstrate_move(boat)     # Output: Sailing ğŸš¤
demonstrate_move(bicycle)  # Output: Pedaling ğŸš²
